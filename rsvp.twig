{% extends "layout.twig" %}

{% block content %}

	<section class="our-story">
		<div class="row">
			<div class="small-12 columns">
				{{widget('pagetitle', 'title', {})|raw}}
			</div>
		</div>
		<div class="row">
			<div class="small-12 columns">
				{{widget('content', 'rsvpcopy', {content:'Thank you for visiting our site!  Let us know if you can make it.'})|raw}}
			
				{{widget('advancedcontactform', 'contact', {
				 	'text': 'Submit',
				  'formTitle': 'Contact Us',
				  'email': 'profile'
				})|raw}}
			</div>
		</div>
		
		<div class="row collapse">
			<div class="small-12 columns">
			    {{zone('main')|raw}}
			</div>
		</div>
	</section>
 
{% endblock %}
