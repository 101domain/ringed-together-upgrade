{% extends "layout.twig" %}

{% block content %}

	<section class="our-story">
		<div class="row">
			<div class="small-12 columns">
				{{widget('pagetitle', 'title', {})|raw}}
			</div>
		</div>
		<div class="row">
			<div class="small-12 columns">
				{{widget('content', 'rsvpcopy', {content:'Thank you for visiting our site!  Let us know if you can make it.'})|raw}}
			
				{{widget('advancedcontactform', 'contact', {
				 	'text': 'Submit',
				  'formTitle': 'Contact Us',
				  'email': 'profile'
				})|raw}}
			</div>
		</div>
		
		<div class="row main-content__banner-spot">
			<div class="small-12 columns">
				<a href="http://weddingchamber.vegas/" target="_new" rel="nofollow">
					<img src="{{asset('images/web-banner-728-x-90.png')}}" alt="Destination Vegas!  Your day, your way.  Wedding Chamber in Las Vegas." />
				</a>
			</div>
		</div> 
		
		<div class="row collapse">
			<div class="small-12 columns">
			    {{zone('main')|raw}}
			</div>
		</div>
	</section>
 
{% endblock %}
